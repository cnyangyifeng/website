<ehcache>

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            maxElementsInMemory="10000"
            eternal="false"
            timeToIdleSeconds="30"
            timeToLiveSeconds="30"
            overflowToDisk="true"
            maxElementsOnDisk="10000000"
            memoryStoreEvictionPolicy="LRU">
    </defaultCache>

</ehcache>
